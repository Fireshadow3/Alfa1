int[] percentageWords(arraylist<string> ht){
  int r[26];            //r= array "alfabeto";c=counter delle lettere totali(si può fare sicuramente meglio di così)
  int c;

  for/while(scorri tutto ht){
    case:
      case a(ciòCheScorreHt.containskey("a")){       //così aggiunge 1 alla posizione della letttra che trova,ma solo 1 per ogni parola,
        r[0]++;                                     //senza aggiungerne di più(va aggiustato) e incrementa il contatore delle parole totali
        c++;
      }
  
      case a(ciòCheScorreHt.containskey("b")){    
        r[1]++;
        c++;
      }   
  }
  for (scorri tutto r){         //0-25
    r=percentage(r[ciòCheScorreR],c);
  }
}

percentage(int n,int d){        //n=numeratore; d=denominatore
    return (n/d)*100;
}
